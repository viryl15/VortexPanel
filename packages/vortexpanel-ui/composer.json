{
  "name": "vortexpanel/vortexpanel-ui",
  "description": "VortexPanel UI (Inertia + Vue) published into the host app.",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "admin", "ui", "inertia", "vue"],
  "homepage": "https://github.com/viryl15/vortexpanel",
  "autoload": {
    "psr-4": {
      "VortexPanel\\UI\\": "src/"
    }
