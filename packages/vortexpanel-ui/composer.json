  "extra": {
    "laravel": {
      "providers": [
        "VortexPanel\\UI\\VortexPanelUiServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "vortexpanel/vortexpanel-core": "*",
    "inertiajs/inertia-laravel": "^0.6.8 || ^1.0"
  },
  "authors": [
    {
      "name": "viryl15",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/viryl15/vortexpanel/issues",
    "source": "https://github.com/viryl15/vortexpanel"
  }
